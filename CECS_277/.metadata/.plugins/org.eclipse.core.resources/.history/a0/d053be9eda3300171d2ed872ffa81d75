package theButtons;

import java.util.Stack;

public class solvePostFix {
	private String s;
	private int result;
	
	solvePostFix(String st){
		s = st;
		if(s != null && s.length() != 0)
			solve();
	}
	
	public void solve(){
		char[] input = s.toCharArray();
		char c;
		Stack<Integer> charStk = new Stack<Integer>();
		Stack<Integer> resultStk = new Stack<Integer>();
		for(int i = 0; i < input.length; i++){
			if(isOperand(input[i])){
				charStk.push(Character.getNumericValue((input[i])));
			}
		}
	}
	
	public boolean isOperand(char c){
		return Character.isDigit(c);
	}
	
	public boolean isOperator(char c){
		switch(c){
		case '+':
		case '-':
		case '*': 
		case '/':
		case '%':
		case '^':
			return true;
		default: 
			return false;
		}
	}
	
	

}
