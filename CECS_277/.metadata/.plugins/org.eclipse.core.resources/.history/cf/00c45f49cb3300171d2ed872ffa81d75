package NotationConversion;

public class inputField {
	private String s;
	
	public inputField(){
		s = "";
	}
	
	public inputField(String st){
		s = st;
	}
	
	public void add(String st){
		s = s + st;
	}
	
	public void delete(){
		if(s != null){
			s = s.substring(0, s.length() - 1);
		}
	}
	
	public void clear(){
		s = "";
	}

}
